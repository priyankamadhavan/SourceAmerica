<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.priyankamadhavan.motivateme">

    <application
        android:allowBackup="true"
        android:icon="@drawable/timer_icon"
        android:supportsRtl="true"
        android:label = "@string/app_name"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Dropdown" />
        <activity android:name=".Failure" />
        <activity android:name=".Success" />
        <activity android:name=".Instructions" />
        <activity android:name=".StartScreen" />
        <activity android:name=".Level" />
        <activity android:name=".Trophies" />
        <activity android:name=".Timer" />
    </application>

</manifest>